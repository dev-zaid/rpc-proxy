{
  "name": "rpc-proxy",
  "version": "1.0.0",
  "description": "RPC proxy translating Parity trace_block calls to Blockscout PostgreSQL",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop rpc-proxy",
    "pm2:restart": "pm2 restart rpc-proxy",
    "pm2:delete": "pm2 delete rpc-proxy",
    "pm2:logs": "pm2 logs rpc-proxy",
    "pm2:status": "pm2 status"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.12.0"
  }
}
